[remote "sayonetech"]
    url = https://github.com/sayonetech/automated-scraper.git
    fetch = +refs/heads/*:refs/remotes/sayonetech/*
[remote "origin"]
    url = git@github.com:huangyingw/sayonetech_automated-scraper.git
    fetch = +refs/heads/*:refs/remotes/origin/*
[push]
    remote = origin
[gsync]
    remote = sayonetech
    target = sayonetech/master
[merge]
    allow = true
[branch "dev"]
    remote = origin
    merge = refs/heads/dev
[branch "master"]
    remote = origin
    merge = refs/heads/master
[gdif]
    ignore = *.gdio.diff
[deploy]
	host = localhost
	path = ~/myproject/git/tools/sayonetech/automated-scraper
